<% if (title) { %>
    <h1><%= title %></h1>
    <% } else {
      res.redirect('/')
    } %>

<%- messages() %>

<% if (errors) { %>
    <ul class="notice">
   <% errors.array().forEach(error => { %>
     <li><%= error.msg %></li>
  <%  }) %>
   </ul>
  <% } %>

 <h2>Welcome <%= locals.accountData.account_firstname %> </h2>
  <p>You're logged in.</p>
  <a id="edit-info-button" title="Click to Edit account information" href="/account/update">
  <h3>Edit Account Information</h3></a>

  <%- checkAccountType %>